    "axios": "^0.21",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "p5": "^1.4.1",
    "postcss": "^8.1.14",
    "prettier": "^2.6.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "vue": "^3.2.31",
